FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (575.25 0 47.69)
);

regions
(
    boxToCell
    {
        box (-100 -100 -100) (100 100 100);

        fieldValues
        (
            volVectorFieldValue U (0 0 0)
        );
    }
);

