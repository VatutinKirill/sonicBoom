FoamFile
{
    version     1912;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 101325;

boundaryField
{
    "(inlet|bottom)"
    {
        type            fixedValue;
        value           uniform 101325;
    }

    "(top|outlet)"
    {
        type            zeroGradient;
    }

    "(back|sym)"
    {
        type            slip;
    }

    "(wing|tail)"
    {
        type            zeroGradient;
    }
}


